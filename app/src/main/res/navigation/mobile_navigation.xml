<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="app.cabill.admin.view.ui.dashboard.DashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="app.cabill.admin.view.ui.messaging.MessagingListFragment"
        android:label="Messaging"
        tools:layout="@layout/fragment_messaging_list" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="app.cabill.admin.view.ui.bill.BillFragment"
        android:label="Bills"
        tools:layout="@layout/fragment_bill" />

    <fragment
        android:id="@+id/navigation_packages"
        android:name="app.cabill.admin.view.ui.packages.PackagesFragment"
        android:label="Packages"
        tools:layout="@layout/fragment_packages" />


    <fragment
        android:id="@+id/navigation_profile"
        android:name="app.cabill.admin.view.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
</navigation>